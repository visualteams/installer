BEEPLUGIN_CLIENT_ID=
BEEPLUGIN_CLIENT_SECRET=
MAIL_SENDER=

# NOTIFICATIONS (PUSH WEB + ANDROID + IOS)
PUBLIC_VAPID_KEY=
