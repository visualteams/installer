# GENERAL
VISUALTEAMS_KEY= # Ask your account manager to get your personal key
APP_DOMAIN=your.domain.ltd # This is the domain where VisualTeams must respond. Don't forget to add in your DNS entries a new rule to this server
#MONGO_URL= # If you want to overide the local database server. Let comment to use the integrated database
#MONGO_URL_OPLOG= # If you want to overide the local database server. Let comment to use the integrated database

# FAST SEARCH
#ELASTIC_URL= # If you want to overide the local elastic server. Let comment to use the integrated search engine
ELASTIC_USERNAME=
ELASTIC_PASSWORD=

# VIDEOCONFERENCE
ETHERPAD_DOMAIN=
ETHERPAD_PASSWORD=changeme

JITSI_DOMAIN=jitsi.domain.ltd
JITSI_APP_ID=
JITSI_SECRET_JWT=

# VIDEOCONFERENCE REPLAYS
JIBRI_USERNAME=
JIBRI_PASSWORD=changeme
#STORAGE_URL= # If you want to overide the local S3 server. Let comment to use the integrated storage
SCALITY_ACCESS_KEY_ID=user # Your Access Key Id. This can be anything if your use the integrated storage (but not empty !)
SCALITY_SECRET_ACCESS_KEY=changeme # Your Secret Access Key. This can be anything if your use the integrated storage (but not empty !)
